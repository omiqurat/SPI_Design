S_00038178 .scope module, "spi_mosi_test" "spi_mosi_test" 2 2;
 .timescale -9 -12;
v02822ef0_0 .var "data_av", 0 0;
v02822f48_0 .var "read_req", 0 0;
v000390a8_0 .var "spi_clk", 0 0;
v00039100_0 .var "spi_cs", 0 0;
v00039158_0 .var "spi_mosi_in", 7 0;
v000391b0_0 .net "spi_mosi_out", 0 0, v02822e40_0;  1 drivers
E_0003b188 .event "end_first_pass";
S_00038248 .scope module, "A1" "spi_mosi" 2 14, 3 2 0, S_00038178;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 1 "spi_mosi_out"
    .port_info 1 /INPUT 1 "spi_cs"
    .port_info 2 /INPUT 1 "spi_clk"
    .port_info 3 /INPUT 1 "data_av"
    .port_info 4 /INPUT 1 "read_req"
    .port_info 5 /INPUT 8 "spi_mosi_in"
P_0003b2f0 .param/l "DSIZE" 0 3 4, +C4<00000000000000000000000000001000>;
v000330f0_0 .var "data", 7 0;
v00033148_0 .net "data_av", 0 0, v02822ef0_0;  1 drivers
v000331a0_0 .var "point", 2 0;
v000331f8_0 .net "read_req", 0 0, v02822f48_0;  1 drivers
v00033250_0 .net "spi_clk", 0 0, v000390a8_0;  1 drivers
v02822d90_0 .net "spi_cs", 0 0, v00039100_0;  1 drivers
v02822de8_0 .net "spi_mosi_in", 7 0, v00039158_0;  1 drivers
v02822e40_0 .var "spi_mosi_out", 0 0;
v02822e98_0 .var "x", 0 0;
E_0003b110 .event posedge, v00033250_0;
    .scope S_00038248;
T_0 ;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v000331a0_0, 0;
    %end;
    .thread T_0;
    .scope S_00038248;
T_1 ;
    %wait E_0003b110;
    %load/vec4 v02822d90_0;
    %load/vec4 v00033148_0;
    %and;
    %load/vec4 v000331f8_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v02822e40_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v02822e98_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v02822d90_0;
    %nor/r;
    %load/vec4 v02822e98_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v000331a0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.4, 4;
    %load/vec4 v02822de8_0;
    %assign/vec4 v000330f0_0, 0;
    %load/vec4 v02822de8_0;
    %parti/s 1, 0, 2;
    %assign/vec4 v02822e40_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v000330f0_0;
    %load/vec4 v000331a0_0;
    %part/u 1;
    %assign/vec4 v02822e40_0, 0;
T_1.5 ;
    %load/vec4 v000331a0_0;
    %addi 1, 0, 3;
    %assign/vec4 v000331a0_0, 0;
    %vpi_call 3 50 "$display", "at time %0d point=%d  ", $time, v000331a0_0 {0 0 0};
    %vpi_call 3 51 "$display", "at time %0d data=%b  ", $time, v000330f0_0 {0 0 0};
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_00038178;
T_2 ;
    %vpi_call 2 23 "$dumpfile", "spi_mosi_waveform_result.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_00038178 {0 0 0};
    %end;
    .thread T_2;
    .scope S_00038178;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000390a8_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00039100_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v02822f48_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v02822ef0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v02822ef0_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v02822ef0_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00039100_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_00038178;
T_4 ;
    %delay 50000, 0;
    %load/vec4 v000390a8_0;
    %inv;
    %store/vec4 v000390a8_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_00038178;
T_5 ;
    %delay 50000, 0;
    %vpi_call 2 47 "$display", "at time %0d spi_clk = %b   spi_cs= %b  data_av=%b read_req=%b spi_mosi_in=%b spi_mosi_out=%b  ", $time, v000390a8_0, v00039100_0, v02822ef0_0, v02822f48_0, v00039158_0, v000391b0_0 {0 0 0};
    %jmp T_5;
    .thread T_5;
    .scope S_00038178;
T_6 ;
    %delay 50000, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v00039158_0, 0, 8;
    %delay 800000, 0;
    %pushi/vec4 12, 0, 8;
    %store/vec4 v00039158_0, 0, 8;
    %delay 2000000, 0;
    %event E_0003b188;
    %vpi_call 2 65 "$finish" {0 0 0};
    %end;
    .thread T_6;
