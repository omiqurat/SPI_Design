    %assign/vec4 v01033278_0, 0;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v01033220_0, 4, 5;
    %pushi/vec4 45, 0, 7;
