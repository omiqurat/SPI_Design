#! c:/iverilog-x86/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_008a2088 .scope module, "clk_div_test" "clk_div_test" 2 2;
 .timescale -9 -12;
v013532b0_0 .var "m_clk", 0 0;
v01353308_0 .var "nrst", 0 0;
v0135e4e0_0 .net "spi_clk", 0 0, v01353200_0;  1 drivers
v0135e538_0 .var "spi_clk_en", 0 0;
S_008a2158 .scope module, "c0" "clk_div" 2 6, 3 2 0, S_008a2088;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 1 "spi_clk"
    .port_info 1 /INPUT 1 "m_clk"
    .port_info 2 /INPUT 1 "spi_clk_en"
    .port_info 3 /INPUT 1 "nrst"
v013531a8_0 .var "counter", 0 3;
v0135eef8_0 .net "m_clk", 0 0, v013532b0_0;  1 drivers
v0135ef50_0 .net "nrst", 0 0, v01353308_0;  1 drivers
v01353200_0 .var "spi_clk", 0 0;
v01353258_0 .net "spi_clk_en", 0 0, v0135e538_0;  1 drivers
E_008a2b28/0 .event negedge, v0135ef50_0;
E_008a2b28/1 .event posedge, v0135eef8_0;
E_008a2b28 .event/or E_008a2b28/0, E_008a2b28/1;
    .scope S_008a2158;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v013531a8_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v01353200_0, 0;
    %end;
    .thread T_0;
    .scope S_008a2158;
T_1 ;
    %wait E_008a2b28;
    %load/vec4 v0135ef50_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v01353200_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v013531a8_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v01353258_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v013531a8_0;
    %cmpi/e 9, 0, 4;
    %jmp/0xz  T_1.4, 4;
    %load/vec4 v01353200_0;
    %nor/r;
    %assign/vec4 v01353200_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v013531a8_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v013531a8_0;
    %addi 1, 0, 4;
    %assign/vec4 v013531a8_0, 0;
T_1.5 ;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_008a2088;
T_2 ;
    %delay 50000, 0;
    %load/vec4 v013532b0_0;
    %inv;
    %store/vec4 v013532b0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_008a2088;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v01353308_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v013532b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0135e538_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_008a2088;
T_4 ;
    %vpi_call 2 25 "$dumpfile", "clk_div_waveform_result.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_008a2088 {0 0 0};
    %end;
    .thread T_4;
    .scope S_008a2088;
T_5 ;
    %vpi_call 2 30 "$display", "time m_clk, spi_clk_en, nrst, spi_clk" {0 0 0};
    %vpi_call 2 31 "$monitor", "at time %0d nrst = %b spi_clk_en = %b m_clk = %b spi_clk=%d", $time, v01353308_0, v0135e538_0, v013532b0_0, v0135e4e0_0 {0 0 0};
    %end;
    .thread T_5;
    .scope S_008a2088;
T_6 ;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01353308_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v01353308_0, 0, 1;
    %delay 10000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01353308_0, 0, 1;
    %delay 2000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v01353308_0, 0, 1;
    %delay 2000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0135e538_0, 0, 1;
    %delay 2000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0135e538_0, 0, 1;
    %delay 2000000, 0;
    %vpi_call 2 57 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "clk_div_test.v";
    "clk_div.v";
