S_01003218 .scope module, "asy_fifo_test" "asy_fifo_test" 2 2;
 .timescale -9 -12;
v010314f0_0 .var "clk", 0 0;
v01031ac8_0 .net "data_out", 119 0, v011031c0_0;  1 drivers
v01031758_0 .var "rinc", 0 0;
v010317b0_0 .var "rst", 0 0;
v01031a70_0 .var "wdata", 7 0;
v01031808_0 .var "winc", 0 0;
E_00ff6d28 .event "end_first_pass";
S_010032e8 .scope module, "A1" "asy_fifo" 2 14, 3 2 0, S_01003218;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 120 "data_out"
    .port_info 1 /INPUT 8 "wdata"
    .port_info 2 /INPUT 1 "winc"
    .port_info 3 /INPUT 1 "clk"
    .port_info 4 /INPUT 1 "rst"
    .port_info 5 /INPUT 1 "rinc"
P_01003c10 .param/l "ASIZE" 0 3 5, +C4<00000000000000000000000000000100>;
P_01003c30 .param/l "DSIZE" 0 3 4, +C4<00000000000000000000000000001000>;
P_01003c50 .param/l "MEMDEPTH" 0 3 18, +C4<00000000000000000000000000010100>;
v00ff63b0_0 .net "clk", 0 0, v010314f0_0;  1 drivers
v011031c0_0 .var "data_out", 119 0;
v01103218 .array "ex_mem", 0 19, 7 0;
v01103270_0 .var/i "i", 31 0;
v011032c8_0 .var "r_empty", 0 0;
v01103320_0 .var "r_pointer", 4 0;
v0110b6b8_0 .var "rdata", 7 0;
v0110b710_0 .net "rinc", 0 0, v01031758_0;  1 drivers
v0110b768_0 .net "rst", 0 0, v010317b0_0;  1 drivers
v0110b7c0_0 .var "w_full", 0 0;
v0110b818_0 .var "w_pointer", 4 0;
v0110b870_0 .net "wdata", 7 0, v01031a70_0;  1 drivers
v0110b8c8_0 .net "winc", 0 0, v01031808_0;  1 drivers
E_00ff7160 .event posedge, v00ff63b0_0;
    .scope S_010032e8;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v01103270_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_010032e8;
T_1 ;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0110b818_0, 0;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v01103320_0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0110b7c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v011032c8_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_010032e8;
T_2 ;
    %wait E_00ff7160;
    %load/vec4 v0110b768_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0110b818_0, 0;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v01103320_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0110b710_0;
    %load/vec4 v01103320_0;
    %pushi/vec4 31, 0, 5;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.2, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v011032c8_0, 0, 1;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v01103320_0, 0;
    %vpi_call 3 47 "$display", "at time %0d rempty=%b", $time, v011032c8_0 {0 0 0};
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0110b8c8_0;
    %load/vec4 v0110b818_0;
    %pushi/vec4 31, 0, 5;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.4, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0110b7c0_0, 0, 1;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0110b818_0, 0;
    %jmp T_2.5;
T_2.4 ;
    %load/vec4 v0110b8c8_0;
    %load/vec4 v0110b7c0_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.6, 8;
    %load/vec4 v0110b818_0;
    %addi 1, 0, 5;
    %store/vec4 v0110b818_0, 0, 5;
    %load/vec4 v0110b870_0;
    %load/vec4 v0110b818_0;
    %pad/u 6;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v01103218, 0, 4;
    %vpi_call 3 64 "$display", "at time %0d w_pointer=%d", $time, v0110b818_0 {0 0 0};
    %jmp T_2.7;
T_2.6 ;
    %load/vec4 v0110b710_0;
    %load/vec4 v011032c8_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.8, 8;
    %load/vec4 v01103320_0;
    %addi 1, 0, 5;
    %store/vec4 v01103320_0, 0, 5;
    %load/vec4 v01103320_0;
    %pad/u 6;
    %ix/vec4 4;
    %load/vec4a v01103218, 4;
    %store/vec4 v0110b6b8_0, 0, 8;
    %vpi_call 3 73 "$display", "at time %0d r_pointer=%d", $time, v01103320_0 {0 0 0};
    %vpi_call 3 74 "$display", "at time %0d rdata=%d", $time, v0110b6b8_0 {0 0 0};
    %load/vec4 v0110b6b8_0;
    %pad/u 120;
    %store/vec4 v011031c0_0, 0, 120;
    %load/vec4 v011031c0_0;
    %load/vec4 v01103270_0;
    %ix/vec4 4;
    %shiftl 4;
    %store/vec4 v011031c0_0, 0, 120;
    %load/vec4 v01103270_0;
    %addi 8, 0, 32;
    %store/vec4 v01103270_0, 0, 32;
    %vpi_call 3 78 "$display", "at time %0d data_out=%b", $time, v011031c0_0 {0 0 0};
T_2.8 ;
T_2.7 ;
T_2.5 ;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_01003218;
T_3 ;
    %vpi_call 2 21 "$dumpfile", "asy_fifo_waveform_result.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_01003218 {0 0 0};
    %end;
    .thread T_3;
    .scope S_01003218;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v010317b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v010317b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v010317b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v010314f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01031808_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01031758_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_01003218;
T_5 ;
    %delay 50000, 0;
    %load/vec4 v010314f0_0;
    %inv;
    %store/vec4 v010314f0_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_01003218;
T_6 ;
    %delay 50000, 0;
    %vpi_call 2 45 "$display", "at time %0d clk = %b   rst= %b  winc=%b rinc=%b wdata=%d data_out=%d ", $time, v010314f0_0, v010317b0_0, v01031808_0, v01031758_0, v01031a70_0, v01031ac8_0 {0 0 0};
    %jmp T_6;
    .thread T_6;
    .scope S_01003218;
T_7 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v01031808_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01031758_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v01031a70_0, 0, 8;
    %delay 100000, 0;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v01031a70_0, 0, 8;
    %delay 150000, 0;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v01031a70_0, 0, 8;
