    %store/vec4 v01031a70_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01031808_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v01031758_0, 0, 1;
    %delay 1000000, 0;
    %event E_00ff6d28;
    %vpi_call 2 81 "$finish" {0 0 0};
